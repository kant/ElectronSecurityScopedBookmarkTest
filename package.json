{
  "name": "electron-webpack-quick-start",
  "description": "App to test electrons security scoped bookmarks",
  "author": "rrosen326@gmail.com",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "rm -rf dist/* && yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "source-map-support": "^0.5.9",
    "vue-loader": "^15.4.2"
  },
  "devDependencies": {
    "electron": "=4.0.0-beta.8",
    "electron-builder": "^20.36.2",
    "electron-webpack": "^2.6.1",
    "electron-webpack-vue": "^2.2.2",
    "vue": "^2.5.17",
    "webpack": "^4.26.0"
  },
  "build": {
    "extends": "node_modules/electron-webpack/out/electron-builder.js",
    "appId": "com.k2companyllc.bookmarktest",
    "productName": "ElectronSecurityScopedBookmarkTestApp",
    "mac": {
      "type": "development",
      "category": "public.app-category.developer-tools",
      "target": ["dir", "mas"],
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.inherit.plist",
      "provisioningProfile": "build/security_scoped_bookmark_pp_mac_development.provisionprofile"    
    }
  },
  "comments": {
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.inherit.plist",
      "provisioningProfile": "build/security_scoped_bookmark_pp_mac_development.provisionprofile"     
  }
}
